    <containerId>56bd55ea-b04b-480d-9e37-5d6f9217dcc3</containerId>
    <code>&lt;div class=&quot;media&quot;&gt;&#xd;
    &lt;a class=&quot;pull-left&quot; href=&quot;/news-events/events/$urlTitle&quot;&gt;&#xd;
		&lt;div class=&quot;date-badge&quot;&gt;&#xd;
      		&lt;span class=&quot;date-month&quot;&gt;$date.format(&apos;MMMM&apos;, ${startDate})&lt;/span&gt;&#xd;
      		&lt;span class=&quot;date-day&quot;&gt;$date.format(&apos;d&apos;, ${startDate})&lt;/span&gt;&#xd;
      	&lt;/div&gt;&#xd;
	&lt;/a&gt;&#xd;
	&lt;div class=&quot;media-body&quot;&gt;&#xd;
		&lt;span class=&quot;label label-default pull-right&quot;&gt;Event&lt;/span&gt;&#xd;
    	&lt;div class=&quot;media-heading&quot;&gt;&lt;a href=&quot;/news-events/events/$urlTitle&quot;&gt;$title&lt;/a&gt;&lt;/div&gt;&#xd;
		&lt;div class=&quot;media-subheading&quot; datetime=&quot;$date.format(&apos;yyyy-M-dd&apos;,$event.startDate)&quot;&gt;&#xd;
			#set($startDateV = $date.format(&apos;MMMM d, h:mm a&apos;, ${startDate}))&#xd;
			#set($endDateV = $date.format(&apos;MMMM d, h:mm a&apos;, ${endDate}))&#xd;
			&#xd;
			#if($startDate == $endDate)&#xd;
				&lt;time&gt;$startDateV&lt;/time&gt;&#xd;
			#elseif($date.format(&apos;EEE, MMMM d yyyy&apos;,$startDateV) == $date.format(&apos;EEE, MMMM d yyyy&apos;,$endDateV))&#xd;
				#if($date.format(&apos;h:mm a&apos;,$startDate) == &apos;12:00 AM&apos; &amp;&amp; $date.format(&apos;h:mm a&apos;,$endDate) == &apos;11:59 PM&apos; )&#xd;
					$date.format(&apos;MMMM d&apos;, ${endDate}) (all day event)&#xd;
				#else&#xd;
					$startDateV - $date.format(&apos;h:mm a&apos;,$endDate)&#xd;
				#end&#xd;
			#else&#xd;
				$startDateV - $endDateV&#xd;
			#end&#xd;
		&lt;/div&gt;&#xd;
	&#xd;
&#xd;
		## This trims the HTML and shorts the description field 200 character&#xd;
		#set($y = $description.replaceAll(&quot;&lt;[^&gt;]*&gt;&quot;, &quot;&quot;))&#xd;
		#set($y = $UtilMethods.prettyShortenString(&quot;$y&quot;, 200))&#xd;
		&lt;p&gt;$y&lt;/p&gt;&#xd;
	&lt;/div&gt;&#xd;
&lt;/div&gt;&#xd;
&#xd;
&lt;hr&gt;</code>
  </com.dotmarketing.beans.ContainerStructure>
