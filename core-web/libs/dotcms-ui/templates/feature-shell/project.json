{
    "projectType": "library",
    "sourceRoot": "libs/dotcms-ui/templates/feature-shell/src",
    "prefix": "dotcms",
    "targets": {
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["coverage/libs/dotcms-ui/templates/feature-shell"],
            "options": {
                "jestConfig": "libs/dotcms-ui/templates/feature-shell/jest.config.ts",
                "passWithNoTests": true
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "options": {
                "lintFilePatterns": [
                    "libs/dotcms-ui/templates/feature-shell/**/*.ts",
                    "libs/dotcms-ui/templates/feature-shell/**/*.html"
                ]
            }
        }
    },
    "tags": ["scope:templates", "type:feature"]
}
