  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "nx test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e dotcms-ui-e2e --headless true --base-url http://localhost:8080",
    "e2e:open": "ng e2e dotcms-ui-e2e --watch --base-url http://localhost:8080",
    "publish:dev": "rm -rf dist;npm run build:prod;cp package.json ./dist/apps/dotcms-ui/package.json;cd dist/apps/dotcms-ui",
    "build:docs:dotcms": "typedoc --options libs/dotcms/typedoc.json;",
    "build:docs:dotcms-models": "typedoc --options libs/dotcms-models/typedoc.json",
    "publish:docs": "gh-pages --dist ./docs --dest ./",
    "docker:test": "npm run docker:build;IMAGE_BASE_NAME=core-web docker-compose -f docker-compose-running-test.yml up",
    "docker:build": "docker build -t core-web .",
    "analyze": "ng build --stats-json;webpack-bundle-analyzer dist/stats.json",
    "docs:json": "compodoc -p ./tsconfig.base.json -e json -d .",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "pretty-quick": "pretty-quick",
    "prepare": "is-ci || pwd && cd ../ && pwd && husky install ui/.husky && cd ./ui",
    "build:dev": "npm run build:dotcms-webcomponents;npx nx build dotcms-ui",
    "build:prod": "npm run build:dotcms-webcomponents;npx nx run dotcms-ui:build:production",
    "build:dotcms-webcomponents": "npx nx build dotcms-webcomponents",
    "build:dotcms-block-editor": "npx nx run dotcms-block-editor:build:production && cat dist/apps/dotcms-block-editor/{generator-runtime,runtime,polyfills,main}.js > dotcms-block-editor.js",
    "storybook:dotcms-webcomponents": "npx nx run dotcms-webcomponents:storybook",
    "storybook:dotcms-ui": "npx nx run dotcms-ui:storybook",
    "storybook:dotcms-block-editor": "npx nx run dotcms-block-editor:storybook",
    "preinstall": "npx npm-force-resolutions"
  },
